{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "member-ordering": false,
    "no-empty-interface": false,
    "no-namespace": false,
    "no-console": [
      true,
      "warning"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-empty": false,
    "quotemark": [
      true,
      "single"
    ]
  }
}
